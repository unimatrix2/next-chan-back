{
  "name": "next-chan-backend",
  "version": "1.0.0",
  "description": "next-chan backend server & API",
  "main": "app.js",
  "scripts": {
    "start": "npm-run-all -s deleteBuild build server",
    "dev": "nodemon --exec babel-node src/app.js",
    "server": "node build/app.js",
    "build": "babel src --out-dir build --copy-files",
    "deleteBuild": "rm -rf build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unimatrix2/next-chan-back.git"
  },
  "author": "Danny Nisenbaum (@unimatrix2)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/unimatrix2/next-chan-back/issues"
  },
  "homepage": "https://github.com/unimatrix2/next-chan-back#readme",
  "dependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.8",
    "@babel/node": "^7.13.0",
    "@babel/preset-env": "^7.13.9",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.4.1",
    "mongoose": "^5.11.18",
    "npm-run-all": "^4.1.5"
  },
  "devDependencies": {
    "eslint": "^7.21.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "nodemon": "^2.0.7"
  }
}
